<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title></title>
    </head>
    <body>
        <h1>Hello Ninja!</h1>
        <div class="display_users">
            <% for (var x in users) { %>
                <h3> Name: <%= users[x].name %> Age: <%= users[x].age %> </h3>
            <% } %>
        </div>
        <form class="" action="/users" method="post">
            <div class="">
                <label for=""> Name: </label>
                <input type="text" name="name">
            </div>
            <div class="">
                <label for=""> Age: </label>
                <input type="number" name="age">
            </div>
            <input type="submit" name="" value="Submit">
        </form>
    </body>
</html>
