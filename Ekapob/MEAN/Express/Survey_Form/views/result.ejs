<html>
    <head>
        <meta charset="utf-8">
       <title>Dojo Survey Results</title>
       <link rel="stylesheet" type="text/css" href="">
    </head>
    <body>
        <div id = "wrapper">
            <div id = "main_content">
                <h3>Submitted Info</h3>
                <div id = "data">
                    <div class = "input_field">
                        <p class = "input_name"> First Name: </p>
                        <p class = "input"> <%= user.first_name %> </p>
                    </div>
                    <div class = "input_field">
                        <p class = "input_name"> First Name: </p>
                        <p class = "input"> <%= user.last_name %> </p>
                    </div>
                    <div class = "input_field">
                        <p class = "input_name"> Alias: </p>
                        <p class = "input"> <%= user.alias %> </p>
                    </div>
                    <div class = "input_field">
                        <p class = "input_name"> Dojo: </p>
                        <p class = "input"> <%= user.dojo %> </p>
                    </div>
                    <div class = "input_field">
                        <p class = "input_name"> Favorite Discipline: </p>
                        <p class = "input"> <%= user.coding_language %> </p>
                    </div>
                    <div class = "input_field">
                        <p class = "input_name"> Posted Reviews: </p>
                        <p class = "input"> <%= user.comment %> </p>
                    </div>
                </div> <!-- end of user_info -->
                <p><a href="{% url 'book_reviews:main' %}">Continue to Main Page</a></p>
            </div> <!-- end of main_content -->
        </div> <!-- end of wrapper -->
    </body>
</html>
